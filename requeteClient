select client.idenclie,client.cateclie,client.typeclie,client.typepiec,replace(replace(client.numpieid,' ',''),'-',''),client.datpieid,replace(replace(replace(replace(replace(client.nomrais,'2 CL',''),'*',''),'°',''),':',''),'2CL','') ,replace(replace(replace(replace(replace(client.prenclie ,'2 CL',''),'*',''),'°',''),':',''),'2CL','') ,client.sexeclie,client.datenais,client.lieunais,client.codenate,client.situfami,client.codsocpr,client.centrc, client.numregco,client.numepate,client.ageneco,client.numevisa,client.nume_teld,client.nume_gsm, client.nume_telb,sectacti, client.datecrea,client.codforju,adresse_link.intitule1, adresse_link.intitule2, adresse_link.intitule3, (Case when (codtypad = 'GN' and adresse_link.intitule4 ) then (select nomquartier from quartiers where	quartiers.codequartier = adresse_link.intitule4 and codequartier <> codevilleloc )   else adresse_link.intitule4 end ), adresse_link.codepost,adresse_link.codepays,   (case when codtypad = 'GN' then (select libellevilleloc from villelocalite where 	codevilleloc = codevill) when codtypad ='GI' then libeville else (select desburpo from bureau_poste where bureau_poste.codburpo = adresse_link.codburpo) end) ,	adresse_link.libeville as ville 	from  client, adresse_link 	where    client.cateclie   in (1,2,3) 	and       client.idenclie = adresse_link.idenclie   and 	adresse_link.codcatad = 'CL' and client.idenclie < 10000000000
